{
  "name": "postcss-plugin-boilerplate",
  "description": "PostCSS plugin boilerplate",
  "repository": "postcss/postcss-plugin-boilerplate",
  "version": "2.0.1",
  "license": "MIT",
  "bin": "cli.js",
  "keywords": [
    "postcss",
    "boilerplate"
  ],
  "scripts": {
    "test": "eslint ."
  },
  "dependencies": {
    "enquirer": "^2.4.1",
    "gogen": "^2.3.0"
  },
  "devDependencies": {
    "clean-publish": "^4.2.0",
    "eslint": "^8.47.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "prettier": {
    "arrowParens": "avoid",
    "jsxSingleQuote": false,
    "quoteProps": "consistent",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  }
}
